<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sarthak.HibernateMapping.Desk" table="desk">
        <id name="uId" column="u_Id" type="int">
            <generator class="increment"></generator>
        </id>
        <property name="locationId" column="location_Id" type="string"/>
        <property name="deskId" column="desk_id" type="int"/>
        <property name="x" column="x" type="int"/>
        <property name="y" column="y" type="int"/>
        <property name="width" column="width" type="int"/>
        <property name="height" column="height" type="int"/>
        <property name="brid" column="brid" type="string"/>
    </class>
</hibernate-mapping>