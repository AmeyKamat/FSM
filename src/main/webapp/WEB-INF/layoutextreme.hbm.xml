<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sarthak.HibernateMapping.LayoutExtremes" table="layout_extremes">
        <id name="layoutId" column="layout_id" type="string">
            <generator class="assigned"></generator>
        </id>
        <property name="minimumX" column="minimum_x" type="int"/>
        <property name="minimumY" column="minimum_y" type="int"/>
        <property name="maximumX" column="maximum_x" type="int"/>
        <property name="maximumY" column="maximum_y" type="int"/>
    </class>
</hibernate-mapping>