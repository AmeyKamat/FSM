<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sarthak.HibernateMapping.TableData" table="table_data">
        <id name="tableUid" column="table_uid" type="int">
            <generator class="increment"></generator>
        </id>
        <property name="layoutId" column="layout_id" type="string"/>
        <property name="tableNumber" column="table_number" type="int"/>
        <property name="topLeftX" column="top_left_x" type="int"/>
        <property name="topLeftY" column="top_left_y" type="int"/>
        <property name="rows" column="rows" type="int"/>
        <property name="columns" column="columns" type="int"/>
        <property name="deskString" column="desk_string" type="string"/>
    </class>
</hibernate-mapping>