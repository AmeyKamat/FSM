<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sarthak.HibernateMapping.OfficeDetails" table="office_details">
        <id name="officeUid" column="office_uid" type="int">
            <generator class="increment"></generator>
        </id>
        <property name="branch" column="branch" type="string"/>
        <property name="country" column="country" type="string"/>
        <property name="city" column="city" type="string"/>
        <property name="floor" column="floor" type="string"/>
    </class>
</hibernate-mapping>